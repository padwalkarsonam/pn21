<!DOCTYPE html>
<html lang="en">

<!-- Basic -->
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<!-- Mobile Metas -->
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<!-- Site Metas -->
<title>PNSV blogs</title>
<meta name="keywords" content="">
<meta name="description" content="">
<meta name="author" content="">

<!-- Site Icons -->
<link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon" />
<link rel="apple-touch-icon" href="images/apple-touch-icon.png">

<!-- Design fonts -->
<link href="https://fonts.googleapis.com/css?family=Droid+Sans:400,700" rel="stylesheet">

<!-- Bootstrap core CSS -->
<link href="/css/bootstrap.css" rel="stylesheet">

<!-- FontAwesome Icons core CSS -->
<link href="/css/font-awesome.min.css" rel="stylesheet">

<!-- Custom styles for this template -->
<link href="../css/style_garden.css" rel="stylesheet">

<!-- Responsive styles for this template -->
<link href="/css/responsive.css" rel="stylesheet">

<!-- Colors for this template -->
<link href="/css/colors.css" rel="stylesheet">

<!-- Version Garden CSS for this template -->
<link href="/css/version/garden.css" rel="stylesheet">

<!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<%- include("../partials/header.ejs") %>
</header>



    <div class="header-section">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="logo">
                        <!--change-1 here pnsv logo will come -->
                        <!-- <a href=""><img src="images/version/garden-logo.png" alt=""></a> -->
                    </div><!-- end logo -->
                </div>
            </div><!-- end row -->
        </div><!-- end header-logo -->
    </div><!-- end header -->



   

    <section class="section wb">
        <div class="container">
            <div class="row">
                <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12">
                    
                    <div class="page-wrapper">
                       
                        <div class="blog-list clearfix">


                            <% if(count==0){%>
                            <h1>No Blogs Yet</h1>
                            <%}%>
                            <% blog_all.forEach(function(blog){ %> 
                            <div class="blog-box row">
                                <div class="col-md-4">
                                    <div class="post-media">
                                        <a href="/blogs/<%=blog._id%>" title="">
                                            <img src="<%=blog.image%>" alt="" class="img-fluid">
                                            <div class="hovereffect"></div>
                                        </a>
                                    </div><!-- end media -->
                                </div><!-- end col -->

                                <div class="blog-meta big-meta col-md-8">
                                    <span class="bg-aqua"><a href="/blogs?category=<%=blog.category%>" title=""><%=blog.category%></a></span>
                                    <h4><a href="/blogs/<%=blog._id%>" title=""><%=blog.title%></a></h4>
                                    <p><%=blog.description.substring(0,250).replace(/(<([^>]+)>)/g, "").replace('&nbsp;',' ')%>.....
                                       </p>
                                    <!-- <small><a href="/blogs/<%=blog._id%>" title=""><i class="fa fa-eye"></i>
                                            1887</a></small> -->
                                    <small><a href="/blogs/<%=blog._id%>" title=""><%=blog.CreatedAT.toISOString().slice(0,10).split("-").reverse().join("-")%></a></small>
                                    <small><a href="#" title=""><%=blog.author.username%></a></small>
                                </div><!-- end meta -->
                            </div><!-- end blog-box -->

                            <hr class="invis">

                         
                            <%})%>
                           

                        </div><!-- end blog-list -->
                        
                    </div><!-- end page-wrapper -->
                    
                    <hr class="invis">

                    <div class="row">
                        <div class="col-md-12">
                            <nav aria-label="Page navigation">
                                <ul class="pagination justify-content-start">
                                    <%for (i = 0; i < count; i++) { %> 
                                        <%if((i%page_num)==0){%>
                                            <li class="page-item"><a class="page-link" href="/blogs?page=<%=Math.round((i/page_num)+1)%>"><%=Math.round((i/page_num)+1)%></a></li>
                                    
                                    <%}%>
                                    
                                    <%if((i/page_num)>=10){%>
                                    <li class="page-item">
                                        <a class="page-link" href="#">Next</a>
                                    </li>
                                
                                    <%}%>
                                    <%}%>
                                </ul>
                            </nav>
                        </div><!-- end col -->
                    </div><!-- end row -->
                </div><!-- end col -->

                <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12">
                    <div class="sidebar">
                        <div class="widget">
                            <h2 class="widget-title">Search</h2>
                            <form class="form-inline search-form">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Search on the site">
                                </div>
                                <button type="submit" class="btn btn-primary"><i class="fa fa-search"></i></button>
                            </form>
                        </div><!-- end widget -->

                        <div class="widget">
                            <h2 class="widget-title">Recent Posts</h2>
                            <div class="blog-list-widget">
                                <div class="list-group">
                                   

                                  <% if(blog_all.length>0){%>
                                    <%for (i = 0; i < 3&& i<blog_all.length; i++) { %> 
                                    <a href="/blogs/<%=blog_all[i]._id%>"
                                        class="list-group-item list-group-item-action flex-column align-items-start">
                                        <div class="w-100 last-item justify-content-between">
                                            <img src="<%=blog_all[i].image%>" alt="" class="img-fluid float-left">
                                            <h5 class="mb-1"><%=blog_all[i].title%></h5>
                                            <small><%=blog_all[i].CreatedAT.toISOString().slice(0,10).split("-").reverse().join("-")%></small>
                                        </div>
                                    </a>
                                    <%}%>
                                        <%}%>
                                    
                                </div>
                            </div><!-- end blog-list -->
                        </div><!-- end widget -->

                        
                        <section class="section-2"></section>
                        <div class="widget">
                            <h2 class="widget-title">Popular Categories</h2>
                            <div class="link-widget">
                                <ul>
                                    <li><a href="/blogs?category=Stock Advisory">Stock Advisory <span>(21)</span></a></li>
                                    <li><a href="/blogs?category=Loans">Loans <span>(15)</span></a></li>
                                    <li><a href="/blogs?category=Insurance">Insurance <span>(31)</span></a></li>
                                    <li><a href="/blogs?category=Bussiness Planning">Bussiness Planning<span>(22)</span></a></li>
                                    <li><a href="/blogs?category=Digital Marketing">Digital Marketing <span>(66)</span></a></li>
                                </ul>
                            </div><!-- end link-widget -->
                        </div><!-- end widget -->
                    </div><!-- end sidebar -->
                </div><!-- end col -->
            </div><!-- end row -->
        </div><!-- end container -->
    </section>


    <div class="container-fluid">
        <div class="row">
            <div class="col-md-4">
                <div class="masonry-box post-media">
                    <img src="https://cdn.pixabay.com/photo/2016/11/23/14/37/blur-1853262__340.jpg" alt="" class="img-fluid">
                    <div class="hovereffect"></div>
                    <div class="shadoweffect">
                        <div class="shadow-desc">
                            <div class="blog-meta">
                                <span class="bg-aqua"><a href="/blogs?category=Stock Advisory" title="">Stock Advisory</a></span>
                                <h4><a href="/blogs?category=Stock Advisory" title="">Get best stock avice blogs</a></h4>
                             
                                <small><a href="#" title="">by Amanda</a></small>
                            </div><!-- end meta -->
                        </div><!-- end shadow-desc -->
                    </div><!-- end shadow -->
                </div><!-- end post-media -->
            </div>
            <div class="col-md-4">
                <div class="masonry-box post-media">
                    <img src="https://cdn.pixabay.com/photo/2017/08/30/07/56/money-2696228_960_720.jpg" alt="" class="img-fluid">
                    <div class="hovereffect"></div>
                    <div class="shadoweffect">
                        <div class="shadow-desc">
                            <div class="blog-meta">
                                <span class="bg-aqua"><a href="/blogs?category=Loans" title="">Loans</a></span>
                                <h4><a href="/blogs?category=Loans" title="">Unable to get loans we have solution</a></h4>
                            
                            </div><!-- end meta -->
                        </div><!-- end shadow-desc -->
                    </div><!-- end shadow -->
                </div><!-- end post-media -->
            </div>
            <div class="col-md-4">
                <div class="masonry-box post-media">
                    <img src="https://cdn.pixabay.com/photo/2017/09/07/08/54/money-2724245__340.jpg" alt="" class="img-fluid">
                    <div class="hovereffect"></div>
                    <div class="shadoweffect">
                        <div class="shadow-desc">
                            <div class="blog-meta">
                                <span class="bg-aqua"><a href="/blogs?category=Insurance" title="">Insurance</a></span>
                                <h4><a href="/blogs?category=Insurance" title="">Confused over large options of Insurance? Get best Insurance in market </a></h4>
                             
                            </div><!-- end meta -->
                        </div><!-- end shadow-desc -->
                    </div><!-- end shadow -->
                </div><!-- end post-media -->
            </div>
        </div>
        <div class="row">
            <div class="col-md-4  ml-auto">
                <div class="masonry-box post-media">
                    <img src="https://cdn.pixabay.com/photo/2015/01/08/18/27/startup-593341__340.jpg" alt="" class="img-fluid">
                    <div class="hovereffect"></div>
                    <div class="shadoweffect">
                        <div class="shadow-desc">
                            <div class="blog-meta">
                                <span class="bg-aqua"><a href="/blogs?category=Bussiness Planning" title="">Bussiness Planning</a></span>
                                <h4><a href="/blogs?category=Bussiness Planning" title="">Plan your Bussiness for best overall growth</a></h4>
                                
                            </div><!-- end meta -->
                        </div><!-- end shadow-desc -->
                    </div><!-- end shadow -->
                </div><!-- end post-media -->
            </div>
            <div class="col-md-4  mr-auto">
                <div class="masonry-box post-media">
                    <img src="https://cdn.pixabay.com/photo/2016/04/13/22/42/seo-1327870__340.png" alt="" class="img-fluid">
                    <div class="hovereffect"></div>
                    <div class="shadoweffect">
                        <div class="shadow-desc">
                            <div class="blog-meta">
                                <span class="bg-aqua"><a href="/blogs?category=Digital Marketing" title="">Digital Marketing</a></span>
                                <h4><a href="/blogs?category=Digital Marketing" title="">Be a step ahead of your competitors </a></h4>
                              
                            </div><!-- end meta -->
                        </div><!-- end shadow-desc -->
                        
                    </div><!-- end shadow -->
                </div><!-- end post-media -->
            </div>
        </div>
    </div>
    
    
    <%- include("../partials/footer.ejs") %>
    <div class="dmtop">Scroll to Top</div>

    </div><!-- end wrapper -->

    <!-- Core JavaScript
    ================================================== -->
   

</body>

</html>